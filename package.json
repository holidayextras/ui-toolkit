    "reactify": "~1.1.0",
    "require-directory": "^2.0.0",
    "requirejs": "~2.1.9"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "eslint": "^0.21.0",
    "eslint-plugin-react": "^2.2.0",
