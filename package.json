  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/holidayextras/ui-toolkit.git"
  },
  "scripts": {
    "postinstall": "./scripts/postinstall.sh",
    "coveralls": "istanbul cover ./node_modules/.bin/_mocha -- --require tests/bootstrap.js --compilers js:babel/register --recursive src/**/*-test.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "mocha --require tests/bootstrap.js --compilers js:babel/register --recursive src/**/*-test.js",
    "lint": "./node_modules/.bin/eslint src --ext .jsx --ext .js --quiet && grunt lintcss && echo 'CSS and JS linting all good! have a üçª'"
  },
  "config": {
    "blanket": {
      "data-cover-only": "src/",
      "data-cover-never": [
        "node_modules",
        "__tests__"
      ],
      "data-cover-loader": "../../../tests/blanket-stub-jsx.js"
    }
  },
  "keywords": [
    "ui",
    "toolkit"
  ],
  "author": {
    "name": "Holiday Extras",
    "email": "uxui@holidayextras.com",
    "url": "http://www.holidayextras.co.uk"
  },
  "bugs": {
